A;AALA,WADA;AAQAW;AACAC,2BADA;AAEAC,6BAFA;AAGAC;AAHA,WARA;AAaArB,mDAbA;AAcAsB;AACAf;AADA;AAdA;AADA;AA5DA;AAiFA;AArGA","names":["BCard","BRow","BCol","BDropdown","BDropdownItem","BButton","components","VueApexCharts","directives","Ripple","props","data","type","revenue_report","revenueReport","chartOptions","chart","stacked","toolbar","show","grid","padding","top","bottom","yaxis","lines","xaxis","categories","labels","style","colors","fontSize","fontFamily","axisTicks","axisBorder","legend","dataLabels","enabled","plotOptions","bar","columnWidth","endingShape","distributed","budgetChart","options","height","zoom","sparkline","stroke","curve","dashArray","width","tooltip"],"sourceRoot":"src/views/dashboard/ecommerce","sources":["EcommerceRevenueReport.vue"],"sourcesContent":["<template>\n  <b-card\n    v-if=\"data\"\n    no-body\n    class=\"card-revenue-budget\"\n  >\n    <b-row class=\"mx-0\">\n      <b-col\n        md=\"8\"\n        class=\"revenue-report-wrapper\"\n      >\n        <div class=\"d-sm-flex justify-content-between align-items-center mb-3\">\n          <h4 class=\"card-title mb-50 mb-sm-0\">\n            Revenue Report\n          </h4>\n          <div class=\"d-flex align-items-center\">\n            <div class=\"d-flex align-items-center mr-2\">\n              <span class=\"bullet bullet-primary svg-font-small-3 mr-50 cursor-pointer\" />\n              <span>Earning</span>\n            </div>\n            <div class=\"d-flex align-items-center ml-75\">\n              <span class=\"bullet bullet-warning svg-font-small-3 mr-50 cursor-pointer\" />\n              <span>Expense</span>\n            </div>\n          </div>\n        </div>\n\n        <!-- chart -->\n        <vue-apex-charts\n          id=\"revenue-report-chart\"\n          type=\"bar\"\n          height=\"230\"\n          :options=\"revenueReport.chartOptions\"\n          :series=\"data.revenueReport.series\"\n        />\n      </b-col>\n\n      <b-col\n        md=\"4\"\n        class=\"budget-wrapper\"\n      >\n        <b-dropdown\n          text=\"2020\"\n          size=\"sm\"\n          class=\"budget-dropdown\"\n          variant=\"outline-primary\"\n        >\n          <b-dropdown-item\n            v-for=\"year in data.years\"\n            :key=\"year\"\n          >\n            {{ year }}\n          </b-dropdown-item>\n        </b-dropdown>\n\n        <h2 class=\"mb-25\">\n          ${{ data.price }}\n        </h2>\n        <div class=\"d-flex justify-content-center\">\n          <span class=\"font-weight-bolder mr-25\">Budget:</span>\n          <span>56,800</span>\n        </div>\n        <vue-apex-charts\n          id=\"budget-chart\"\n          type=\"line\"\n          height=\"80\"\n          :options=\"budgetChart.options\"\n          :series=\"data.budgetChart.series\"\n        />\n\n        <b-button\n          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n          variant=\"primary\"\n        >\n          Increase Budget\n        </b-button>\n      </b-col>\n    </b-row>\n  </b-card>\n</template>\n\n<script>\nimport {\n  BCard, BRow, BCol, BDropdown, BDropdownItem, BButton,\n} from 'bootstrap-vue'\nimport VueApexCharts from 'vue-apexcharts'\nimport { $themeColors } from '@themeConfig'\nimport Ripple from 'vue-ripple-directive'\n\nexport default {\n  components: {\n    VueApexCharts,\n    BDropdown,\n    BDropdownItem,\n    BCard,\n    BButton,\n    BRow,\n    BCol,\n  },\n  directives: {\n    Ripple,\n  },\n  props: {\n    data: {\n      type: Object,\n      default: () => {},\n    },\n  },\n  data() {\n    return {\n      revenue_report: {},\n      revenueReport: {\n        chartOptions: {\n          chart: {\n            stacked: true,\n            type: 'bar',\n            toolbar: { show: false },\n          },\n          grid: {\n            padding: {\n              top: -20,\n              bottom: -10,\n            },\n            yaxis: {\n              lines: { show: false },\n            },\n          },\n          xaxis: {\n            categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep'],\n            labels: {\n              style: {\n                colors: '#6E6B7B',\n                fontSize: '0.86rem',\n                fontFamily: 'Montserrat',\n              },\n            },\n            axisTicks: {\n              show: false,\n            },\n            axisBorder: {\n              show: false,\n            },\n          },\n          legend: {\n            show: false,\n          },\n          dataLabels: {\n            enabled: false,\n          },\n          colors: [$themeColors.primary, $themeColors.warning],\n          plotOptions: {\n            bar: {\n              columnWidth: '17%',\n              endingShape: 'rounded',\n            },\n            distributed: true,\n          },\n          yaxis: {\n            labels: {\n              style: {\n                colors: '#6E6B7B',\n                fontSize: '0.86rem',\n                fontFamily: 'Montserrat',\n              },\n            },\n          },\n        },\n      },\n      // budget chart\n      budgetChart: {\n        options: {\n          chart: {\n            height: 80,\n            toolbar: { show: false },\n            zoom: { enabled: false },\n            type: 'line',\n            sparkline: { enabled: true },\n          },\n          stroke: {\n            curve: 'smooth',\n            dashArray: [0, 5],\n            width: [2],\n          },\n          colors: [$themeColors.primary, '#dcdae3'],\n          tooltip: {\n            enabled: false,\n          },\n        },\n      },\n    }\n  },\n}\n</script>\n"]}]}