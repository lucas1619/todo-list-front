l=\"value\"\n          class=\"mb-md-0 mb-2\"\n          min=\"1\"\n          max=\"100\"\n        >\n          <template #increment>\n            <feather-icon icon=\"ChevronUpIcon\" />\n          </template>\n          <template #decrement>\n            <feather-icon icon=\"ChevronDownIcon\" />\n          </template>\n        </b-form-spinbutton>\n      </b-col>\n\n      <!-- Demo 2 : Week -->\n      <b-col md=\"6\">\n        <b-form-spinbutton\n          v-model=\"day\"\n          min=\"0\"\n          max=\"6\"\n          wrap\n          :formatter-fn=\"dayFormatter\"\n        >\n          <template #increment>\n            <feather-icon icon=\"ChevronRightIcon\" />\n          </template>\n          <template #decrement>\n            <feather-icon icon=\"ChevronLeftIcon\" />\n          </template>\n        </b-form-spinbutton>\n      </b-col>\n    </b-row>\n\n    <template #code>\n      {{ codeSlots }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code'\nimport {\n  BFormSpinbutton, BRow, BCol, BCardText,\n} from 'bootstrap-vue'\nimport { codeSlots } from './code'\n\nexport default {\n  components: {\n    BFormSpinbutton,\n    BCardCode,\n    BCardText,\n    BRow,\n    BCol,\n  },\n  data() {\n    return {\n      value: 50,\n      codeSlots,\n      day: 0,\n      days: ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'],\n    }\n  },\n  methods: {\n    dayFormatter(value) {\n      return this.days[value]\n    },\n  },\n}\n</script>\n"]}]}