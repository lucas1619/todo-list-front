ss=\"modal-actions\">\n        <feather-icon\n          icon=\"MinusIcon\"\n          class=\"cursor-pointer\"\n          @click=\"$emit('update:shall-show-email-compose-modal', false)\"\n        />\n        <feather-icon\n          icon=\"Maximize2Icon\"\n          class=\"ml-1 cursor-pointer\"\n        />\n        <feather-icon\n          icon=\"XIcon\"\n          class=\"ml-1 cursor-pointer\"\n          @click=\"discardEmail\"\n        />\n      </div>\n    </template>\n\n    <!-- Modal Footer -->\n    <template #modal-footer>\n      <!-- Footer: Left Content -->\n      <div>\n        <b-dropdown\n          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n          split\n          text=\"Send\"\n          variant=\"primary\"\n          right\n          @click=\"sendEmail\"\n        >\n          <b-dropdown-item>\n            Schedule Send\n          </b-dropdown-item>\n        </b-dropdown>\n        <feather-icon\n          icon=\"PaperclipIcon\"\n          size=\"17\"\n          class=\"ml-2 cursor-pointer\"\n        />\n      </div>\n\n      <!-- Footer: Right Content -->\n      <div>\n\n        <!-- Dropdown: More Actions -->\n        <b-dropdown\n          variant=\"link\"\n          no-caret\n          toggle-class=\"p-0\"\n          right\n        >\n          <template #button-content>\n            <feather-icon\n              icon=\"MoreVerticalIcon\"\n              size=\"17\"\n              class=\"text-body\"\n            />\n          </template>\n\n          <b-dropdown-item>\n            Add Label\n          </b-dropdown-item>\n\n          <b-dropdown-item>\n            Plain Text Mode\n          </b-dropdown-item>\n\n          <b-dropdown-divider />\n\n          <b-dropdown-item>\n            Print\n          </b-dropdown-item>\n          <b-dropdown-item>\n            Check Spelling\n          </b-dropdown-item>\n        </b-dropdown>\n\n        <!-- Discard Compose -->\n        <feather-icon\n          icon=\"TrashIcon\"\n          size=\"17\"\n          class=\"ml-75 cursor-pointer\"\n          @click=\"discardEmail\"\n        />\n      </div>\n    </template>\n\n    <!-- Modal: Body -->\n    <b-form>\n\n      <!-- Field: To -->\n      <div class=\"compose-mail-form-field\">\n        <label\n          for=\"email-to\"\n          class=\"form-label\"\n        >To: </label>\n        <v-select\n          v-model=\"composeData.to\"\n          :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n          multiple\n          label=\"name\"\n          class=\"flex-grow-1 email-to-selector\"\n          :close-on-select=\"false\"\n          :options=\"emailToOptions\"\n          input-id=\"email-to\"\n        >\n\n          <template #option=\"{ avatar, name }\">\n            <b-avatar\n              size=\"sm\"\n              :src=\"avatar\"\n            />\n            <span class=\"ml-50\"> {{ name }}</span>\n          </template>\n\n          <template #selected-option=\"{ avatar, name }\">\n            <b-avatar\n              size=\"sm\"\n              class=\"border border-white\"\n              :src=\"avatar\"\n            />\n            <span class=\"ml-50\"> {{ name }}</span>\n          </template>\n        </v-select>\n        <span\n          class=\"cursor-pointer\"\n          @click=\"showCcField=!showCcField\"\n        >Cc</span>\n        <span\n          class=\"ml-1 cursor-pointer\"\n          @click=\"showBccField=!showBccField\"\n        >Bcc</span>\n      </div>\n\n      <!-- Field: Cc (Hidden Initially) -->\n      <div\n        v-show=\"showCcField\"\n        class=\"compose-mail-form-field\"\n      >\n        <label for=\"email-cc\">CC: </label>\n        <v-select\n          v-model=\"composeData.cc\"\n          :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n          multiple\n          label=\"name\"\n          class=\"flex-grow-1 email-cc-selector\"\n          :close-on-select=\"false\"\n          :options=\"emailToOptions\"\n          input-id=\"email-cc\"\n        >\n\n          <template #option=\"{ avatar, name }\">\n            <b-avatar\n              size=\"sm\"\n              :src=\"avatar\"\n            />\n            <span class=\"ml-50\"> {{ name }}</span>\n          </template>\n\n          <template #selected-option=\"{ avatar, name }\">\n            <b-avatar\n              size=\"sm\"\n              class=\"border border-white\"\n              :src=\"avatar\"\n            />\n            <span class=\"ml-50\"> {{ name }}</span>\n          </template>\n        </v-select>\n      </div>\n\n      <!-- Field: Bcc (Hidden Initially) -->\n      <div\n        v-show=\"showBccField\"\n        class=\"compose-mail-form-field\"\n      >\n        <label for=\"email-bcc\">Bcc </label>\n        <v-select\n          v-model=\"composeData.bcc\"\n          :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n          multiple\n          label=\"name\"\n          class=\"flex-grow-1 email-bcc-selector\"\n          :close-on-select=\"false\"\n          :options=\"emailToOptions\"\n          input-id=\"email-bcc\"\n        >\n\n          <template #option=\"{ avatar, name }\">\n            <b-avatar\n              size=\"sm\"\n              :src=\"avatar\"\n            />\n            <span class=\"ml-50\"> {{ name }}</span>\n          </template>\n\n          <template #selected-option=\"{ avatar, name }\">\n            <b-avatar\n              size=\"sm\"\n              class=\"border border-white\"\n              :src=\"avatar\"\n            />\n            <span class=\"ml-50\"> {{ name }}</span>\n          </template>\n        </v-select>\n      </div>\n\n      <!-- Field: Subject -->\n      <div class=\"compose-mail-form-field\">\n        <label for=\"email-subject\">Subject: </label>\n        <b-form-input\n          id=\"email-subject\"\n          v-model=\"composeData.subject\"\n        />\n      </div>\n\n      <!-- Field: Message - Quill Editor -->\n      <div class=\"message-editor\">\n        <quill-editor\n          id=\"quil-c