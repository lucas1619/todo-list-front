tle\"\n          >\n            <b-media no-body>\n              <b-media-aside\n                vertical-align=\"center\"\n                class=\"mr-50\"\n              >\n                <b-avatar\n                  href=\"javascript:void(0)\"\n                  size=\"24\"\n                  :src=\"blogDetail.blog.avatar\"\n                />\n              </b-media-aside>\n              <b-media-body>\n                <small class=\"text-muted mr-50\">by</small>\n                <small>\n                  <b-link class=\"text-body\">{{ blogDetail.blog.userFullName }}</b-link>\n                </small>\n                <span class=\"text-muted ml-75 mr-50\">|</span>\n                <small class=\"text-muted\">{{ blogDetail.blog.createdTime }}</small>\n              </b-media-body>\n            </b-media>\n            <div class=\"my-1 py-25\">\n              <b-link\n                v-for=\"tag in blogDetail.blog.tags\"\n                :key=\"tag\"\n              >\n                <b-badge\n                  pill\n                  class=\"mr-75\"\n                  :variant=\"tagsColor(tag)\"\n                >\n                  {{ tag }}\n                </b-badge>\n              </b-link>\n            </div>\n            <!-- eslint-disable vue/no-v-html -->\n            <div\n              class=\"blog-content\"\n              v-html=\"blogDetail.blog.content\"\n            />\n\n            <!-- user commnets -->\n            <b-media\n              v-for=\"user in blogDetail.blog.UserComment\"\n              :key=\"user.avatar\"\n              no-body\n            >\n              <b-media-aside>\n                <b-avatar\n                  size=\"60\"\n                  :src=\"user.avatar\"\n                />\n              </b-media-aside>\n              <b-media-body>\n                <h6 class=\"font-weight-bolder\">\n                  {{ user.fullName }}\n                </h6>\n                <b-card-text>\n                  {{ user.comment }}\n                </b-card-text>\n              </b-media-body>\n            </b-media>\n            <!-- eslint-enable -->\n            <hr class=\"my-2\">\n\n            <div class=\"d-flex align-items-center justify-content-between\">\n              <div class=\"d-flex align-items-center\">\n                <div class=\"d-flex align-items-center mr-1\">\n                  <b-link class=\"mr-50\">\n                    <feather-icon\n                      icon=\"MessageSquareIcon\"\n                      size=\"21\"\n                      class=\"text-body\"\n                    />\n                  </b-link>\n                  <b-link>\n                    <div class=\"text-body\">\n                      {{ kFormatter(blogDetail.blog.comments) }}\n                    </div>\n                  </b-link>\n                </div>\n                <div class=\"d-flex align-items-center\">\n                  <b-link class=\"mr-50\">\n                    <feather-icon\n                      size=\"21\"\n                      icon=\"BookmarkIcon\"\n                      class=\"text-body\"\n                    />\n                  </b-link>\n                  <b-link>\n                    <div class=\"text-body\">\n                      {{ kFormatter(blogDetail.blog.bookmarked) }}\n                    </div>\n                  </b-link>\n                </div>\n              </div>\n\n              <!-- dropdown -->\n              <div class=\"blog-detail-share\">\n                <b-dropdown\n                  variant=\"link\"\n                  toggle-class=\"p-0\"\n                  no-caret\n                  right\n                >\n                  <template #button-content>\n                    <feather-icon\n                      size=\"21\"\n                      icon=\"Share2Icon\"\n                      class=\"text-body\"\n                    />\n                  </template>\n                  <b-dropdown-item\n                    v-for=\"icon in socialShareIcons\"\n                    :key=\"icon\"\n                    href=\"#\"\n                  >\n                    <feather-icon\n                      :icon=\"icon\"\n                      size=\"18\"\n                    />\n                  </b-dropdown-item>\n                </b-dropdown>\n              </div>\n              <!--/ dropdown -->\n            </div>\n          </b-card>\n        </b-col>\n        <!--/ blogs -->\n\n        <!-- blog comment -->\n        <b-col\n          id=\"blogComment\"\n          cols=\"12\"\n          class=\"mt-2\"\n        >\n          <h6 class=\"section-label\">\n            Comment\n          </h6>\n          <b-card\n            v-for=\"(comment,index) in blogDetail.comments\"\n            :key=\"index\"\n          >\n            <b-media no-body>\n              <b-media-aside class=\"mr-75\">\n                <b-avatar\n                  :src=\"comment.avatar\"\n                  size=\"38\"\n                />\n              </b-media-aside>\n              <b-media-body>\n                <h6 class=\"font-weight-bolder mb-25\">\n                  {{ comment.userFullName }}\n                </h6>\n                <b-card-text>{{ comment.commentedAt }}</b-card-text>\n                <b-card-text>\n                  {{ comment.commentText }}\n                </b-card-text>\n                <b-link>\n                  <div class=\"d-inline-flex align-items-center\">\n                    <feather-icon\n                      icon=\"CornerUpLeftIcon\"\n                      size=\"18\"\n                      class=\"mr-50\"\n                    />\n                    <span>Reply</span>\n                  </div>\n                </b-link>\n              </b-media-body>\n            </b-media>\n          </b-card>\n        </b-col>\n        <!--/ blog comment -->\n\n        <!-- Leave a Blog Comment -->\n        <b-col\n          cols=\"12\"\n          class=\"mt-2\"\n        >\n          <h6 class=\"section-label\">\n            Leave a Comment\n          </h6>\n          <b-card>\n            <b-form>\n              <b-row>\n                <b-col sm=\"6\">\n                  <b-form-group class=\"mb-2\">\n                    <b-form-input\n                      name=\"name\"\n                      placeholder=\"Name\"\n                    />\n                  </b-form-group>\n                </b-col>\n                <b-col sm=\"6\">\n                  <b-form-group class=\"mb-2\">\n                    <b-form-input\n                      name=\"email\"\n                      type=\"email\"\n                      placeholder=\"Email\"\n                    />\n                  </b-form-group>\n                </b-col>\n                <b-col sm=\"6\">\n                  <b-form-group class=\"mb-2\">\n                    <b-form-input\n                      name=\"website\"\n                      placeholder=\"Website\"\n                    />\n                  </b-form-group>\n                </b-col>\n                <b-col cols=\"12\">\n                  <b-form-group class=\"mb-2\">\n                    <b-form-textarea\n                      name=\"textarea\"\n                      rows=\"4\"\n                      placeholder=\"Website\"\n                    />\n                  </b-form-group>\n                </b-col>\n                <b-col cols=\"12\">\n                  <b-form-checkbox\n                    id=\"checkbox-1\"\n                    v-model=\"commentCheckmark\"\n                    name=\"checkbox-1\"\n                    class=\"mb-2\"\n                  >\n                    Save my name, email, and website in this browser for the next time I comment.\n                  </b-form-checkbox>\n                </b-col>\n                <b-col cols=\"12\">\n                  <b-button\n                    v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n                    variant=\"primary\"\n                  >\n                    Post Comment\n                  </b-button>\n                </b-col>\n              </b-row>\n            </b-form>\n          </b-card>\n        </b-col>\n        <!--/ Leave a Blog Comment -->\n      </b-row>\n      <!--/ blogs -->\n    </div>\n    <!--/ content -->\n\n    <!-- sidebar -->\n    <div\n      slot=\"sidebar\"\n      class=\"blog-sidebar py-2 py-lg-0\"\n    >\n      <!-- input search -->\n      <b-form-group class=\"blog-search\">\n        <b-input-group class=\"input-group-merge\">\n          <b-form-input\n            id=\"search-input\"\n            v-model=\"search_query\"\n            placeholder=\"Search here\"\n          />\n          <b-input-group-append\n            class=\"cursor-pointer\"\n            is-text\n          >\n            <feather-icon\n              icon=\"SearchIcon\"\n            />\n          </b-input-group-append>\n        </b-input-group>\n      </b-form-group>\n      <!--/ input search -->\n\n      <!-- recent posts -->\n      <div class=\"blog-recent-posts mt-3\">\n        <h6 class=\"section-label mb-75\">\n          Recent Posts\n        </h6>\n        <b-media\n          v-for=\"(recentpost,index) in blogSidebar.recentPosts\"\n          :key=\"recentpost.img\"\n          no-body\n          :class=\"index? 'mt-2':''\"\n        >\n          <b-media-aside class=\"mr-2\">\n            <b-link>\n              <b-img\n                :src=\"recentpost.img\"\n                :alt=\"recentpost.img.slice(6)\"\n                width=\"100\"\n                rounded\n                height=\"70\"\n              />\n            </b-link>\n          </b-media-aside>\n          <b-media-body>\n            <h6 class=\"blog-recent-post-title\">\n              <b-link class=\"text-body-heading\">\n                {{ recentpost.title }}\n              </b-link>\n            </h6>\n            <span class=\"text-muted mb-0\">\n              {{ recentpost.createdTime }}\n            </span>\n          </b-media-body>\n        </b-media>\n      </div>\n      <!--/ recent posts -->\n\n      <!-- categories -->\n      <div class=\"blog-categories mt-3\">\n        <h6 class=\"section-label mb-1\">\n          Categories\n        </h6>\n\n        <div\n          v-for=\"category in blogSidebar.categories\"\n          :key=\"category.icon\"\n          class=\"d-flex justify-content-start align-items-center mb-75\"\n        >\n          <b-link>\n            <b-avatar\n              rounded\n              size=\"32\"\n              :variant=\"tagsColor(category.category)\"\n              class=\"mr-75\"\n            >\n              <feather-icon\n                :icon=\"category.icon\"\n                size=\"16\"\n              />\n            </b-avatar>\n          </b-link>\n          <b-link>\n            <div class=\"blog-category-title text-body\">\n              {{ category.category }}\n            </div>\n          </b-link>\n        </div>\n      </div>\n      <!--/ categories -->\n    </div>\n  </content-with-sidebar>\n</template>\n\n<script>\nimport {\n  BFormInput, BMedia, BAvatar, BMediaAside, BMediaBody, BImg, BLink, BFormGroup, BInputGroup, BInputGroupAppend,\n  BCard, BRow, BCol, BBadge, BCardText, BDropdown, BDropdownItem, BForm, BFormTextarea, BFormCheckbox, BButton,\n} from 'bootstrap-vue'\nimport Ripple from 'vue-ripple-directive'\nimport { kFormatter } from '@core/utils/filter'\nimport ContentWithSidebar from '@core/layouts/components/content-with-sidebar/ContentWithSidebar.vue'\n\nexport default {\n  components: {\n    BFormInput,\n    BMedia,\n    BAvatar,\n    BMediaAside,\n    BMediaBody,\n    BLink,\n    BCard,\n    BRow,\n    BCol,\n    BFormGroup,\n    BInputGroup,\n    BInputGroupAppend,\n    BImg,\n    BBadge,\n    BCardText,\n    BDropdown,\n    BForm,\n    BDropdownItem,\n    BFormTextarea,\n    BFormCheckbox,\n    BButton,\n    ContentWithSidebar,\n  },\n  directives: {\n    Ripple,\n  },\n  data() {\n    return {\n      search_query: '',\n      commentCheckmark: '',\n      blogDetail: [],\n      blogSidebar: {},\n      socialShareIcons: [\n        'GithubIcon',\n        'GitlabIcon',\n        'FacebookIcon',\n        'TwitterIcon',\n        'LinkedinIcon',\n      ],\n    }\n  },\n  created() {\n    this.$http.get('/blog/list/data/detail').then(res => { this.blogDetail = res.data })\n    this.$http.get('/blog/list/data/sidebar').then(res => { this.blogSidebar = res.data })\n  },\n  methods: {\n    kFormatter,\n    tagsColor(tag) {\n      if (tag === 'Quote') return 'light-info'\n      if (tag === 'Gaming') return 'light-danger'\n      if (tag === 'Fashion') return 'light-primary'\n      if (tag === 'Video') return 'light-warning'\n      if (tag === 'Food') return 'light-success'\n      return 'light-primary'\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\">\n@import '@core/scss/vue/pages/page-blog.scss';\n</style>\n"]}]}