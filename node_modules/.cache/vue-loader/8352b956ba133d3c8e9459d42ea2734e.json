{"remainingRequest":"/home/lucas/Desktop/test/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/lucas/Desktop/test/src/views/forms/form-element/vue-select/VueSelectMultiple.vue?vue&type=script&lang=js&","dependencies":[{"path":"/home/lucas/Desktop/test/src/views/forms/form-element/vue-select/VueSelectMultiple.vue","mtime":1611055082000},{"path":"/home/lucas/Desktop/test/node_modules/cache-loader/dist/cjs.js","mtime":1646760974730},{"path":"/home/lucas/Desktop/test/node_modules/babel-loader/lib/index.js","mtime":1646760975446},{"path":"/home/lucas/Desktop/test/node_modules/cache-loader/dist/cjs.js","mtime":1646760974730},{"path":"/home/lucas/Desktop/test/node_modules/vue-loader/lib/index.js","mtime":1646760975666}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["VueSelectMultiple.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4JA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"VueSelectMultiple.vue","sourceRoot":"src/views/forms/form-element/vue-select","sourcesContent":["<template>\n  <b-card title=\"Multiple Select\">\n    <b-row>\n      <b-col lg=\"6\">\n        <b-card-code\n          title=\"Basic Multiple Select\"\n          class=\"border\"\n        >\n          <b-card-text>\n            <span>Equivalent to the </span>\n            <code>multiple</code>\n            <span> attribute on a </span>\n            <code>&lt;select&gt;</code>\n            <span> input.</span>\n          </b-card-text>\n\n          <v-select\n            v-model=\"selected\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            multiple\n            label=\"title\"\n            :options=\"option\"\n          />\n\n          <template #code>\n            {{ codeVueMultiBasic }}\n          </template>\n        </b-card-code>\n      </b-col>\n      <b-col lg=\"6\">\n        <b-card-code\n          title=\"Select with Icon\"\n          class=\"border\"\n        >\n          <b-card-text>\n            <span>The current option within the dropdown, contained within </span><code>&lt;li&gt;</code>\n            <span>.</span>\n          </b-card-text>\n\n          <v-select\n            v-model=\"selected1\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            multiple\n            :options=\"books\"\n            label=\"title\"\n          >\n            <template #option=\"{ title, icon }\">\n              <feather-icon\n                :icon=\"icon\"\n                size=\"16\"\n                class=\"align-middle mr-25\"\n              />\n              <span> {{ title }}</span>\n            </template>\n          </v-select>\n\n          <template #code>\n            {{ codeVueMultiIcon }}\n          </template>\n        </b-card-code>\n      </b-col>\n      <b-col lg=\"6\">\n        <b-card-code\n          title=\"vue-select pushTags\"\n          class=\"border\"\n        >\n          <b-card-text>\n            To allow input that's not present within the options, set the taggable prop to true.\n          </b-card-text>\n\n          <v-select\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            multiple\n            taggable\n            push-tags\n            placeholder=\"Add Options\"\n            label=\"title\"\n          />\n\n          <template #code>\n            {{ codeMultiPush }}\n          </template>\n        </b-card-code>\n      </b-col>\n      <b-col lg=\"6\">\n        <b-card-code\n          title=\"Limiting the Number of Selections\"\n          class=\"border mb-0\"\n        >\n          <b-card-text>\n            <code>selectable</code>\n            <span>\n              can also be used a bit more creatively to limit the number selections that can be made within the component.\n              In this case, the user can select any author, but may only select a maximum of three books.\n            </span>\n          </b-card-text>\n\n          <v-select\n            v-model=\"selected2\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            multiple\n            placeholder=\"Choose up to 3 books!\"\n            label=\"title\"\n            :options=\"books\"\n            :selectable=\"() => selected2.length < 3\"\n          />\n\n          <template #code>\n            {{ codeVueSelectionLimit }}\n          </template>\n        </b-card-code>\n      </b-col>\n      <b-col lg=\"6\">\n        <b-card-code\n          title=\"Vue-Select direction\"\n          class=\"border mb-0\"\n        >\n          <b-card-text><span>Sets RTL support. Accepts </span><code>ltr, rtl, auto</code>.</b-card-text>\n\n          <div class=\"mb-2\">\n            <b-form-radio\n              v-model=\"dir\"\n              name=\"some-radios\"\n              value=\"ltr\"\n              inline\n            >\n              LRT\n            </b-form-radio>\n            <b-form-radio\n              v-model=\"dir\"\n              name=\"some-radios\"\n              value=\"rtl\"\n              inline\n            >\n              RTL\n            </b-form-radio>\n          </div>\n\n          <v-select\n            v-model=\"selected3\"\n            :dir=\"dir\"\n            multiple\n            label=\"title\"\n            :options=\"books\"\n          />\n\n          <template #code>\n            {{ codeVueDir }}\n          </template>\n        </b-card-code>\n      </b-col>\n    </b-row>\n  </b-card>\n</template>\n\n<script>\nimport {\n  BRow, BCol, BFormRadio, BCard, BCardText,\n} from 'bootstrap-vue'\nimport BCardCode from '@core/components/b-card-code'\nimport vSelect from 'vue-select'\nimport {\n  codeVueMultiBasic, codeVueMultiIcon, codeMultiPush, codeVueSelectionLimit, codeVueDir,\n} from './code'\n\nexport default {\n  components: {\n    BRow,\n    BCol,\n    BFormRadio,\n    BCardText,\n    BCard,\n    BCardCode,\n    vSelect,\n  },\n  data() {\n    return {\n      codeVueMultiBasic,\n      codeVueMultiIcon,\n      codeMultiPush,\n      codeVueSelectionLimit,\n      codeVueDir,\n      dir: 'ltr',\n      selected: [\n        { title: 'Square' },\n        { title: 'Romboid' },\n      ],\n      selected1: [\n        {\n          title: 'Command',\n          icon: 'CommandIcon',\n        },\n      ],\n      selected2: [],\n      selected3: [{\n        title: 'Database',\n        icon: 'DatabaseIcon',\n      }],\n      options: [],\n      option: [\n        { title: 'Square' },\n        { title: 'Rectangle' },\n        { title: 'Rombo' },\n        { title: 'Romboid' },\n        { title: 'Trapeze' },\n        { title: 'Triangle' },\n        { title: 'Polygon' },\n      ],\n      books: [\n        {\n          title: 'Database',\n          icon: 'DatabaseIcon',\n        },\n        {\n          title: 'Codepen',\n          icon: 'CodepenIcon',\n        },\n        {\n          title: 'Aperture ',\n          icon: 'ApertureIcon',\n        },\n        {\n          title: 'Command',\n          icon: 'CommandIcon',\n        },\n      ],\n    }\n  },\n}\n</script>\n"]}]}