nsion -->\n    <label\n      for=\"extension\"\n      class=\"mt-1\"\n    >Accept specific image formats by extension</label>\n    <b-form-file\n      id=\"extension\"\n      accept=\".jpg, .png, .gif\"\n    />\n\n    <template #code>\n      {{ codeLimiting }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code'\nimport { BFormFile, BCardText } from 'bootstrap-vue'\nimport { codeLimiting } from './code'\n\nexport default {\n  components: {\n    BCardCode,\n    BFormFile,\n    BCardText,\n  },\n  data() {\n    return {\n      codeLimiting,\n    }\n  },\n}\n</script>\n"]}]}