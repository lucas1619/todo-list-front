        </b-form-radio>\n            <b-form-radio value=\"readonly\">\n              Readonly\n            </b-form-radio>\n            <b-form-radio value=\"normal\">\n              Normal\n            </b-form-radio>\n          </b-form-radio-group>\n        </b-form-group>\n\n        <!-- calendar -->\n        <b-calendar\n          id=\"ex-disabled-readonly\"\n          :disabled=\"disabled\"\n          :readonly=\"readonly\"\n        />\n      </div>\n    </div>\n\n    <template #code>\n      {{ codeStates }}\n    </template>\n  </b-card-code>\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code'\nimport {\n  BFormGroup, BFormRadioGroup, BFormRadio, BCalendar, BCardText,\n} from 'bootstrap-vue'\nimport { codeStates } from './code'\n\nexport default {\n  components: {\n    BCardCode,\n    BFormGroup,\n    BFormRadioGroup,\n    BFormRadio,\n    BCalendar,\n    BCardText,\n  },\n  data() {\n    return {\n      state: 'disabled',\n      codeStates,\n    }\n  },\n  computed: {\n    disabled() {\n      return this.state === 'disabled'\n    },\n    readonly() {\n      return this.state === 'readonly'\n    },\n  },\n}\n</script>\n"]}]}